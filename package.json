    "@koa/cors": "^3.3.0",
    "@koa/router": "^10.1.1",
    "@types/joi": "^14.3.1",
    "@types/koa": "^2.13.4",
    "@types/node": "^17.0.38",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "ioredis": "^5.0.6",
    "joi": "^17.9.1",
    "joi-to-interface": "^1.2.1",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-static-router": "^1.5.0",
    "lodash": "^4.17.21",
    "log4js": "^6.5.2",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "npm": "^8.12.1",
    "sequelize": "^6.20.1",
    "sql-ddl-to-json-schema": "^4.0.6",
    "ts-node": "^10.9.1"
  },
